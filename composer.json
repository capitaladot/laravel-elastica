{
    "name": "xxstop/laravel-elastica",
    "description": "elastica for laravel",
    "homepage": "https://github.com/xxstop/laravel-elastica",
    "license": "MIT",
    "authors": [
        {
            "name": "wish",
            "email": "wish@baffedu.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "elasticsearch/elasticsearch": "^6.0",
        "ruflin/elastica": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Xxstop\\LaravelElastica\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Xxstop\\LaravelElastica\\ServiceProvider"
            ],
            "aliases": {
                "Elasticsearch": "Xxstop\\LaravelElastica\\Facade"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
